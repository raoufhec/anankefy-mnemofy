{
	"name": "mnemofy",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"theme:build": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ci": "vitest --coverage",
		"test:ui": "playwright test",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@material/dom": "^14.0.0",
		"@material/elevation": "^14.0.0",
		"@playwright/test": "^1.28.1",
		"@smui/banner": "^7.0.0-beta.0",
		"@smui/button": "^7.0.0-beta.0",
		"@smui/common": "^7.0.0-beta.0",
		"@smui/dialog": "^7.0.0-beta.0",
		"@smui/drawer": "^7.0.0-beta.0",
		"@smui/form-field": "^7.0.0-beta.0",
		"@smui/icon-button": "^7.0.0-beta.0",
		"@smui/list": "^7.0.0-beta.0",
		"@smui/menu": "^7.0.0-beta.0",
		"@smui/select": "^7.0.0-beta.0",
		"@smui/switch": "^7.0.0-beta.0",
		"@smui/tab": "^7.0.0-beta.0",
		"@smui/tab-bar": "^7.0.0-beta.0",
		"@smui/textfield": "^7.0.0-beta.0",
		"@smui/top-app-bar": "^7.0.0-beta.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@testing-library/svelte": "^3.2.2",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-c8": "^0.26.2",
		"dexie": "^3.2.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.2",
		"jsdom": "^20.0.3",
		"moment": "^2.29.4",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.57.1",
		"smui-theme": "^7.0.0-beta.0",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"tinyqueue": "^2.0.3",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"uuid": "^9.0.0",
		"vite": "^4.0.0",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"husky": {
		"hooks": {
			"pre-commit": "npm run format",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
